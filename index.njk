    <h2>Writing</h2>

      {% for post in collections.post | reverse %}
        {% if loop.index0 < 3 %}
          <div class="column">
            <div class="card">
              <header class="card-header">
                <p class="card-header-title">
                  {{ post.data.title }}
                </p>
              </header>
              <div class="card-content">
                <div class="content">
                  {{ post.data.description }}
                </div>
              </div>

                <a href="{{ post.url }}">Read article</a>

            </div>
          </div>
          {% endif %}
      {% endfor %}




